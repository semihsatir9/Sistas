@model IEnumerable<Sistas.Models.userdata>

@{
    ViewBag.Title = "Display";
}

<h2>Display</h2>


<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.data1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.data2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.data3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.data4)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.data1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data4)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
